# Phase 1 Agent System Configuration
# Enhanced lazy loading system for core content pipeline agents

# Agent Loading Control
AGENT_LOADING_ENABLED=true
PROGRESSIVE_LOADING=true
AGENT_LOADING_TIMEOUT=30

# Memory Management
AGENT_MEMORY_LIMIT_MB=100
MAX_CONCURRENT_AGENTS=3

# Railway Production Settings (existing)
DATABASE_URL=postgresql://user:password@host:port/database
OPENAI_API_KEY=your_openai_api_key_here

# Additional Performance Settings
LANGCHAIN_API_KEY=your_langchain_api_key_here
LANGCHAIN_TRACING_V2=true
LANGCHAIN_PROJECT=credilinq-agent-phase1

# Security Settings
ADMIN_EMAIL=admin@credilinq.com
SECRET_KEY=auto_generated_on_startup
JWT_SECRET=auto_generated_on_startup